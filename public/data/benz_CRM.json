    "tables": [
      {
        "table_name": "Dealerships",
        "purpose": "Manages basic information of Mercedes-Benz dealers in Korea.",
        "columns": [
          {"column_name": "dealership_id", "type": "INTEGER", "is_primary_key": true, "description": "Dealership unique ID"},
          {"column_name": "dealership_name", "type": "VARCHAR", "description": "Dealership name"},
          {"column_name": "dealership_name_kr", "type": "VARCHAR", "description": "Dealership name in Korean"},
          {"column_name": "location", "type": "VARCHAR", "description": "Dealership location"},
          {"column_name": "contact_person", "type": "VARCHAR", "description": "Dealership contact person name"},
          {"column_name": "contact_email", "type": "VARCHAR", "description": "Dealership contact person email"}
        ],
