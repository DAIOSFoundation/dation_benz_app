      {
        "table_name": "CustomerWaitlist",
        "purpose": "Manages information of customers waiting to purchase specific vehicle models.",
        "columns": [
          {"column_name": "waitlist_id", "type": "INTEGER", "is_primary_key": true, "description": "Waitlist unique ID"},
          {"column_name": "dealership_id", "type": "INTEGER", "is_foreign_key": true, "references_table": "Dealerships", "references_column": "dealership_id", "description": "Dealership ID where customer applied for waitlist"},
          {"column_name": "customer_name", "type": "VARCHAR", "description": "Customer name"},
          {"column_name": "customer_phone", "type": "VARCHAR", "description": "Customer contact number"},
          {"column_name": "customer_email", "type": "VARCHAR", "description": "Customer email"},
          {"column_name": "model_id", "type": "INTEGER", "is_foreign_key": true, "references_table": "VehicleModels", "references_column": "model_id", "description": "Vehicle model ID applied for waitlist"},
          {"column_name": "waitlist_date", "type": "DATE", "description": "Waitlist application date"},
          {"column_name": "priority_level", "type": "INTEGER", "description": "Priority level (1: High, 2: Medium, 3: Low)"},
          {"column_name": "estimated_delivery", "type": "DATE", "description": "Estimated delivery date"},
          {"column_name": "status", "type": "VARCHAR", "description": "Waitlist status (Waiting, Allocated, Cancelled)"}
